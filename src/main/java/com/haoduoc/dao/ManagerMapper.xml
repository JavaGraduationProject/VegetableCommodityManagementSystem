<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haoduoc.dao.ManagerMapper">
    <!--基类型做参数传递可以不写parameterType-->

    <!--根据账号查询,返回一个HaoDuoCai-->
    <select id="queryByGmaccount" resultType="Manager">
        select * from manager
        where gmaccount = #{gmaccount}
    </select>

</mapper>
